echo @on

REM Kill all existing instance of chrome 
taskkill /F /IM chrome.exe /T
REM directory path where chrome.exe is located
set chromeLocation="C:\Program Files (x86)\Google\Chrome\Application"
cd %chromeLocation%
cd c:
start chrome.exe --start-fullscreen https://www.c2000.local --load-extension="C:\Users\Administrator.WIN-BNBGPS1502I\AppData\Local\Google\Chrome\User Data\Default\EExtensions\amddgdnlkmohapieeekfknakgdnpbleb\1.0.0"
